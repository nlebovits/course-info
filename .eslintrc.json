{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "L": "readonly",
    "Promise": "readonly"
  },
  "rules": {
    "object-curly-spacing": "off",
    "max-len": "off",
    "require-jsdoc": "off"
  }
}